# Authentication Key
AUTH_KEY=your_auth_key_here
# Wallet Private Key (Base58 encoded)
MY_PRIVATE_KEY=your_private_key_here
# Solana RPC Endpoint
RPC_ENDPOINT=https://mainnet.helius-rpc.com/?api-key=your_rpc_api_key_here
# Solana WebSocket Endpoint
RPC_WEBSOCKET=wss://mainnet.helius-rpc.com/?api-key=your_rpc_api_key_here
# Token Symbol to Trade
TOKEN_SYMB=WSOL
# Pool Size Constraints
MIN_POOL_SIZE=2
MAX_POOL_SIZE=100
# Buy Amount
BUY_AMOUNT=0.0012
# Commitment Level
COMMITMENT_LEVEL=finalized
# Snipe List Configuration
USE_SNIPEDLIST=false
SNIPE_LIST_REFRESH_INTERVAL=30000
# Mint Renouncement Check
MINT_IS_RENOUNCED=true
# Auto Sell Configuration
AUTO_SELL=true
AUTO_SELL_DELAY=20000
MAX_SELL_RETRIES=5
# Profit and Loss Settings
TAKE_PROFIT=80
STOP_LOSS=30
# BirdEye API Key
BIRDEYE_APIKEY=your_birdeye_apikey_here
# Log Level
LOG_LEVEL=debug
# Enable Buy
ENABLE_BUY=true
